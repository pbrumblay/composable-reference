{
	"name": "composable-reference",
	"version": "0.1.0",
	"private": true,
	"license": "AGPL-3.0",
	"type": "module",
	"scripts": {
		"dev": "MQTT_WEBSOCKET=false harperdb-nextjs dev",
		"build": "harperdb-nextjs build",
		"start": "harperdb-nextjs start",
		"lint": "next lint --dir .",
		"next:build": "SKIP_USE_CACHE_DURING_BUILD=1 next build",
		"format": "prettier --write .",
		"deploy": "npx -y dotenv-cli harperdb deploy . restart=rolling replicated=true"
	},
	"dependencies": {
		"@harperdb/nextjs": "^1.1.5",
		"next": "^16.1.6",
		"react": "^19.2.4",
		"react-dom": "^19.2.4",
		"typescript": "^5.7.0"
	},
	"devDependencies": {
		"@harperdb/code-guidelines": "^0.0.2",
		"@types/node": "^22.10.0",
		"@types/react": "^18.3.0",
		"@types/react-dom": "^18.3.0",
		"eslint": "^10.0.1",
		"eslint-config-next": "^0.2.4",
		"eslint-config-prettier": "^10.1.5",
		"prettier": "^3.5.3"
	},
	"prettier": "@harperdb/code-guidelines/prettier",
	"engines": {
		"node": ">=22"
	},
	"harperdb": {
		"component": true,
		"resources": [
			"resources/index.ts"
		]
	}
}
